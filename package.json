  "author": "NAV",
  "license": "MIT",
  "scripts": {
    "ensure-test-features": "node ./test-utils/ensure-test-features.js",
    "blackbox": "npm run up && wait-on http://localhost:4243/health/is-ready && mocha blackbox-test.js",
    "serve": "nodemon",
    "test": "npm run kafka-up && mocha \"src/**/*.spec.js\"",
    "up": "docker-compose -f docker-compose.yml up -d",
    "kafka-up": "docker-compose -f docker-compose-kafka.yml up -d"
  },
  "dependencies": {
    "ajv": "^6.12.4",
    "axios": "^0.23.0",
    "fastify": "^3.22.0",
    "fastify-cors": "^4.1.0",
    "fastify-formbody": "^5.1.0",
    "fastify-metrics": "^6.0.3",
    "fastify-static": "^4.4.2",
    "geoip-lite": "^1.4.2",
    "isbot": "^3.3.4",
    "json-schema-library": "^5.3.0",
    "kafkajs": "^1.14.0",
    "pino": "^6.6.0",
    "prom-client": "^12.0.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "mocha": "^9.1.2",
    "moxios": "^0.4.0",
    "nock": "^13.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "pino-pretty": "^4.2.1",
    "uuid": "^7.0.3",
    "wait-on": "^6.0.0",
    "yaml": "^1.10.0"
  }
