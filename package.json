{
  "name": "amplitude-proxy",
  "version": "1.0.0",
  "main": "serve.js",
  "description": "Application that whitelists and forwards traffic to amplitudes servers.",
  "repository": "https://github.com/navikt/amplitude-proxy.git",
  "author": "NAV",
  "license": "MIT",
  "scripts": {
    "ensure-test-keyfile": "node ./test-utils/ensure-test-keyfile.js",
    "blackbox": "node ./test-utils/ensure-test-ingresses.js && npm run up && mocha blackbox-test.js",
    "serve": "nodemon",
    "test": "npm run kafka-up && mocha \"src/**/*.spec.js\"",
    "up": "docker-compose -f docker-compose.yml up -d --build",
    "kafka-up": "docker-compose -f docker-compose-kafka.yml up -d --build"
  },
  "dependencies": {
    "ajv": "^6.12.4",
    "axios": "^0.20.0",
    "fastify": "^3.3.0",
    "fastify-cors": "^4.1.0",
    "fastify-formbody": "^5.0.0",
    "fastify-metrics": "^6.0.2",
    "fastify-static": "^3.2.0",
    "geoip-lite": "^1.4.2",
    "isbot": "^3.0.8",
    "json-schema-library": "^4.1.4",
    "kafkajs": "^1.14.0",
    "pino": "^6.6.0",
    "prom-client": "^12.0.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "mocha": "^7.2.0",
    "moxios": "^0.4.0",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.2.1",
    "uuid": "^7.0.3",
    "yaml": "^1.10.0"
  }
}
